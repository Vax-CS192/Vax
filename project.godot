; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "VirusGenerator",
"language": "GDScript",
"path": "res://Scenes/Shared/VirusGenerator.gd"
} ]
_global_script_class_icons={
"VirusGenerator": ""
}

[application]

config/name="Vax"
run/main_scene="res://Scenes/Shared/Session.tscn"
config/icon="res://icon.png"

[autoload]

Profile="*res://Scenes/Shared/Profile.gd"
PersistentScenes="*res://Scenes/Shared/PersistentScenes.gd"
EventSystem="*res://Scenes/Shared/EventSystem.gd"

[display]

window/size/width=2960
window/size/height=1440
window/stretch/mode="2d"
window/stretch/aspect="keep"

[global]

window=false
touch=false

[importer_defaults]

bitmap={
"create_from": 0,
"threshold": 0.5
}

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

environment/default_environment="res://default_env.tres"
